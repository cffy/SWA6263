{
	"routes": [{
		"route": "/*",
		"allowedRoles": ["authenticated"],
	}],
	"responseOverrides": {
		"401": {
			"statusCode": 302,
			"redirect": "/.auth/login/aad"
		}
	},
	"networking": {
		"allowedIpRanges": ["AzureFrontDoor.Backend"]
	},
	"navigationFallback": {
		"rewrite": "/index.html"
	}
}
